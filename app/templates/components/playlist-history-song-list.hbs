{{#each tracks as |track|}}
  <TrackInfo
    @trackTitle={{track.catalogEntry.title}}
    @ensembleName={{track.catalogEntry.ensemble.name}}
    @conductorName={{track.catalogEntry.conductor.name}}
    @composerName={{track.catalogEntry.composer.name}}
    @startTime={{track.startTime}}
  />
{{/each}}

{{yield}}