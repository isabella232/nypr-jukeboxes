<div class="flex-grid-thirds">
  <div class="col" id="wqxr-logo">{{nypr-svg className="site-header-title-logo" icon="wqxr-log-frq"}}</div>
  <div class="col">
    <p class="live-nyc-txt">LIVE FROM NYC</p>
  </div>
  <div class="col donate-button"> <a href="https://www.wqxr.org/support/" target="_blank"><button>Donate</button></a>
  </div>
</div>

{{#sticky-element as |state|}}
<div id="listen-playhistory">
  <div class="new-donate"></div>
  {{horizontal-nav
    navRoot='listen'
    defaultSlug='listen'
    channelSlug='listen'
    links=links
    class=(if state.isSticky "is-sticky")
    id=(if state.isSticky "sticky-donate")
    activeTabIndex=activeTabIndex}}
</div>
{{/sticky-element}}